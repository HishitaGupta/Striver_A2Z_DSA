{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++.exe build active file",
            "command": "C:/MinGW/bin/g++.exe",
            "args": ["${file}", "-o", "${fileDirname}/${fileBasenameNoExtension}.exe"],
            "group": { "kind": "build", "isDefault": true },
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}